@* MatchTemplate -> Content -> Table*@

@model IEnumerable<ChemodartsWebApp.Models.Match>

@foreach (var item in Model) {
    <tr>
        <partial name="MatchTemplate/Content/Columns/Players" model="@item"/>
        <partial name="MatchTemplate/Content/Columns/Score" model="@item"/>
        <partial name="MatchTemplate/Content/Columns/Venue" model="@item"/>
        <partial name="MatchTemplate/Content/Columns/Time" model="@item"/>
    </tr>
}