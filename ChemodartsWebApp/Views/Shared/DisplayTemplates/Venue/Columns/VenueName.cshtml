@model Venue

@switch(ViewData["RenderType"])
{
    case "Header":
        <th>
            @Html.DisplayNameFor(model => Model.VenueName)
        </th>
        break;
    case "Content":
        <td>
            <a cd-route="Venue" cd-action="Details" cd-route-venueId="@Model.VenueId">@Html.DisplayFor(model => Model.VenueName)</a>
        </td>
        break;
    default:
        goto case "Content";
}