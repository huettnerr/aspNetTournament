@model Match

@switch(ViewData["RenderType"])
{
    case "Header":
        <th>
            @Html.DisplayNameFor(model => model.Group.GroupName)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.MatchOrderValue)
        </th>*@
        break;
    case "Content":
        <td>
            <a cd-route="Group" cd-action="Index" cd-route-tournamentId="@Model.Group.Round.TournamentId" cd-route-roundId="@Model.Group.RoundId" cd-route-groupId="@Model.GroupId">
                @Html.DisplayFor(model => Model.Group.GroupName)
            </a>
        </td>
        @*<td>
            @Html.DisplayFor(model => Model.MatchOrderValue)
        </td>*@
        break;
    default:
        goto case "Content";
}