@model Match

@switch(ViewData["RenderType"])
{
    case "Header":
        <th>
            @Html.DisplayNameFor(model => model.Seed1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Seed2)
        </th>
        break;
    case "Content":
        @Html.DisplayFor(m => Model.Seed1, "Seed/Columns/SeedPlayerName")
        @Html.DisplayFor(m => Model.Seed2, "Seed/Columns/SeedPlayerName")
        break;
    default:
        goto case "Content";
}