@model IEnumerable<ChemodartsWebApp.Models.Player>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PlayerName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PlayerDartname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PlayerContactData)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PlayerInterpret)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PlayerSong)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PlayerName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PlayerDartname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PlayerContactData)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PlayerInterpret)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PlayerSong)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PlayerId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PlayerId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PlayerId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
