@*@model IEnumerable<ChemodartsWebApp.Models.Group>*@
@model Round
@{var GroupDummy = Model.Groups.FirstOrDefault(); }

@{
    ViewData["Title"] = "Home Page";
}

@if (User.Identity.IsAuthenticated){ <a asp-controller="Tournament" asp-action="GroupCreate" asp-route-roundId="@Model.RoundId">Gruppe hinzufügen</a> <hr />}

<div>
    <a asp-action="Index">Back</a>
</div>

<div class="text-center">
    <h1 class="display-4"><b>@Model.RoundName</b> - @Html.DisplayFor(model => model.Modus)</h1>
</div>

<partial name="Rounds/Round_@Model.Modus.ToString()" model="@Model"/>